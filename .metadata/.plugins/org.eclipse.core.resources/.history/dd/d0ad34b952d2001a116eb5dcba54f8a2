package com.practice;

class Mammal{
	String name = "Furry";
	String makeNosie() {
		return "generic noise";
	}
}

class Zebra extends Mammal{
	String name = "stripes";
	String makeNoise() {
		return "Bray";
	}
}

public class ZooKeeper {
	
	void go() {
		Mammal m = new Zebra();
		System.out.println(m.name + m.makeNosie());
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new ZooKeeper().go();
	}

}
