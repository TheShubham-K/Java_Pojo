package com.poly;

public class ForTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 10;
		int[] arr = new int[3]; 
		arr[0] = 10;
		arr[1] = 20;
		arr[2] = 30;
		
		for(int j = 0;i<arr.length;j++) {
			System.out.println("Items of the Array are : "+arr[j]);
		}
	}

}
