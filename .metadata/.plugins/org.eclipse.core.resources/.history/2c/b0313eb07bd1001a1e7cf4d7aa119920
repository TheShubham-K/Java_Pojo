package com.poly;

class X{
	public void test1() {
		System.out.println("Test 1 implemented in X!!");
	}
}

class Y extends X{
	public void test1() {
		System.out.println("Test 1 implemented in X!!");
	}
}

class Z extends Y{
	public void test1() {
		System.out.println("Test 1 implemented in X!!");
	}
}

public class Polymorp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		X x = new X();
		x.test1();
		System.out.println("-------------------------------------");
		Y y = new Y();
		y.test1();
		System.out.println("-------------------------------------");
		Z z = new Z();
		z.test1();
	}

}
